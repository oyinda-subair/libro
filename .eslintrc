{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "semi": 0,
    "no-multiple-empty-lines": 0,
    "object-shorthand": 0,
    "padded-blocks": 0,
    "no-unused-vars": [1, {
      "vars": "local",
      "args": "none"
    }],
    "max-len": [1, 80, 2, {
      "ignoreComments": true
    }],
    "import/no-unresolved": [2, {
      "commonjs": true,
      "amd": true
    }],
    "import/named": 2,
    "import/namespace": 2,
    "import/default": 2,
    "import/export": 2,
    "strict": 0
  },
  "settings": {
    "import/resolver": "node"
  }
}